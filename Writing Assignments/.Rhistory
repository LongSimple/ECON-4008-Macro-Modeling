####   Clear memory and load needed packages
rm(list = ls())
library(plyr)
library(knitr)
library(readr)
library(kableExtra)
library(stargazer)
SumStats <- ldply(c("Minimum", "Maximum", "Mean", "Median", "Mode", "Standard Deviation"))
Statval <- ldply(c("x", "x", "x", "x", "x", "x"))
SumStats <- cbind(SumStats, Statval)
kable(SumStats, digits = 5, booktabs = T, caption = "Summary Statistics for Bitcoin Returns")
SumStats <- ldply(c("Minimum", "Maximum", "Mean", "Median", "Standard Deviation"))
Statval <- ldply(c(-354.70, 598.30, .27, .29, .045008))
SumStats <- cbind(SumStats, Statval)
kable(SumStats, digits = 5, booktabs = T, caption = "Summary Statistics for Bitcoin Returns (Percent)")
